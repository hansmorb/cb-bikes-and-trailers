  "homepage": "https://github.com/hansmorb/cb-bikes-and-trailers#readme",
  "devDependencies": {
    "@10up/cypress-wp-utils": "^0.2.0",
    "@wordpress/env": "^8.9.0",
    "cypress": "^13.3.1",
    "grunt": "~1.6.1",
    "grunt-wp-i18n": "~1.0.3"
  }
}
